public class ex5 {

		public static void main(String[] args) {
	int s=375;
	int a=4,b=5,c=6;
	int t;
	t=((s-(a+b+c))/12);
	int sa,o,p;
	sa=(t*3);
	o=(t*4);
	p=(t*5);

		System.out.println("the value of x is:"+sa);
		System.out.println("the value of y is:"+o);
		System.out.println("the value of z is:"+p);

		}

	}
