import java.util.*;
import java.lang.*;
public class ex2 {
	

	 public static void main(String args[])

	    {

	        int x,y;

	        //given data x+y=25;x-y=13

	        x=(38/2);

	        System.out.println(x);

	        y=25-x;

	        System.out.println(y);

	        //To find product od these two numbers

	        int z=x*y;

	        System.out.println(z);

	    }
}
